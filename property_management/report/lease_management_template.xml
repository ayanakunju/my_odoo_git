<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template id="form_lease_management_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <div class="text-center">
                                <h1>Lease Management</h1>
                            </div>
                            <h3>Customer:
                                <span t-field="o.name"/>
                            </h3>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>
