<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="view_warning_message_wizard_form" model="ir.ui.view">
         <field name="name">warning.message.wizard.form</field>
         <field name="model">warning.message.wizard</field>
         <field name="arch" type="xml">
             <form string="Warning Message">
                 <field name="message" string="Quantity is out of the acceptable range. Do you want to accept or reject?" readonly="1"/>
                 <footer>
                     <button string="Accept" type="object" name="action_accept" class="btn-primary"/>
                     <button string="Don't Accept" special="cancel" name="action_reject" class="btn-danger" />
                 </footer>
                 <sheet>
                 </sheet>
             </form>
         </field>
     </record>
 </odoo>
